{
  "name": "kdd-ui",
  "version": "0.5.5",
  "private": false,
  "author": "zhufengli",
  "description": "A Component Library for Vue.js.",
  "style": "lib/kdd-ui.css",
  "main": "./src/index.js",
  "keywords": [
